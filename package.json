{
  "name": "jobscope",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "fix:bin-perms": "bash ./scripts/fix-bin-perms.sh",
    "predev": "npm run fix:bin-perms",
    "dev": "astro dev",
    "prebuild": "npm run fix:bin-perms",
    "build": "ASTRO_TELEMETRY_DISABLED=1 astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "ASTRO_TELEMETRY_DISABLED=1 astro check",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pretest": "npm run fix:bin-perms",
    "test": "node ./node_modules/vitest/vitest.mjs run",
    "test:watch": "node ./node_modules/vitest/vitest.mjs",
    "test:coverage": "node ./node_modules/vitest/vitest.mjs run --coverage"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.0",
    "@astrojs/vercel": "^8.1.0",
    "@supabase/supabase-js": "^2.95.3",
    "astro": "^5.17.1",
    "eslint": "^9.22.0",
    "eslint-plugin-astro": "^1.2.2",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "typescript-eslint": "^8.30.0",
    "vitest": "^3.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.57.1",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^3.2.4"
  }
}
